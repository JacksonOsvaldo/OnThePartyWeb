<div class="wrapper">
  <h2><strong>Faça seu cadastro</strong></h2>
  <p>Preencha os campos abaixo para poder criar eventos, enviar fotos e comentários de festas</p>
  <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), html: {id: "ContactForm"}) do |f| %>
  <%= f.error_messages %>
  <div>
    <div class="wrapper">
      <span><%= f.label :name %></span>
      <%= f.email_field :name, class: :input %>
    </div>
  </div>

  <div>
    <div class="wrapper">
      <span><%= f.label :email %></span>
      <%= f.email_field :email, class: :input %>
    </div>
  </div>

  <div>
    <div class="wrapper">
      <span><%= f.label :password %></span>
      <%= f.password_field :password, class: :input %>
    </div>
  </div>

  <div>
    <div class="wrapper">
      <span><%= f.label :password_confirmation %></span>
      <%= f.password_field :password_confirmation, class: :input %>
    </div>
  </div>

  <a href="#" style="float:left" class="button" onClick="$('#ContactForm').submit()">Criar minha conta</a>
  <% end %>
</div>
